{
    "name": "tiknil/file-vault",
    "description": "",
    "keywords": [
        "laravel",
        "encrypt",
        "decrypt",
        "encryption",
        "decryption",
        "cbc",
        "php",
        "file",
        "file-vault"
    ],
    "homepage": "https://github.com/tiknil/file-vault",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Costin Soare",
            "email": "soarecostin@gmail.com",
            "role": "Original Developer"
        },
        {
            "name": "Giorgio Balduzzi",
            "email": "balduzzi.giorgio@tiknil.com",
            "role": "Fork Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "5.8.* || 6.*.* || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tiknil\\FileVault\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiknil\\FileVault\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tiknil\\FileVault\\FileVaultServiceProvider"
            ],
            "aliases": {
                "FileVault": "Tiknil\\FileVault\\Facades\\FileVault"
            }
        }
    }
}
